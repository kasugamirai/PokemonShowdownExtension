    {
      "matches": [
        "http://china.psim.us/*",
        "https://china.psim.us/*",
        "http://47.94.147.145.psim.us/*",
        "http://replay.pokemonshowdown.com/*",
        "https://replay.pokemonshowdown.com/*",
        "https://play.pokemonshowdown.com/*",
        "http://smogtours.psim.us/*",
        "https://smogtours.psim.us/*",
        "http://localhost:8000/*",
        "http://g410178v57.qicp.vip-80.psim.us/*"
      ],
      "js": ["main.js"]
    }
  ],
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}
